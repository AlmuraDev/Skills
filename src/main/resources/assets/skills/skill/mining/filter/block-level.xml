<?xml version="1.0" encoding="utf-8"?>
<droplet xmlns:skills="skills" spec="1.0.0">
  <filters>
    <all id="stone-block-level">
      <filter id="stone-block"/>
      <skills:level>
        <any>
          <equal-to value="1"/>
          <greater-than value="1"/>
        </any>
      </skills:level>
    </all>
    <all id="coal-ore-block-level">
      <filter id="coal-ore-block"/>
      <skills:level>
        <any>
          <equal-to value="10"/>
          <greater-than value="10"/>
        </any>
      </skills:level>    
    </all>
    <all id="iron-ore-block-level">
      <filter id="iron-ore-block"/>
      <skills:level>
        <any>
          <equal-to value="20"/>
          <greater-than value="20"/>
        </any>
      </skills:level>
    </all>
    <all id="gold-ore-block-level">
      <filter id="gold-ore-block"/>
      <skills:level>
        <any>
          <equal-to value="30"/>
          <greater-than value="30"/>
        </any>
      </skills:level>
    </all>
    <all id="diamond-ore-block-level">
      <filter id="diamond-ore-block"/>
      <skills:level>
        <any>
          <equal-to value="40"/>
          <greater-than value="40"/>
        </any>
      </skills:level>
    </all>
    <all id="obsidian-block-level">
      <filter id="obsidian-block"/>
      <skills:level>
        <any>
          <equal-to value="50"/>
          <greater-than value="50"/>
        </any>
      </skills:level>
    </all>
    <all id="emerald-ore-block-level">
      <filter id="emerald-ore-block"/>
      <skills:level>
        <any>
          <equal-to value="60"/>
          <greater-than value="60"/>
        </any>
      </skills:level>
    </all>
  </filters>
</droplet>
